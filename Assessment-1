//Question-1: Get a number from the user,add 2 to that number and print result.

#include <stdio.h>
int main() {
int x,y;
printf("Enter Number:");
scanf("%d",&x);
y=x+2;
printf("Result=%d",y);                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                 
