}

// Question-2: Get a number from the user,substract 5 from that number and print result.

#include <stdio.h>
int main() {
int x,y;
printf("Enter Number:");
scanf("%d",&x);
y=x-5;
printf("Result= %d",y);
     return 0;
     }
// Question-3: Get a number from the user,multiply that number by 3 and print result

